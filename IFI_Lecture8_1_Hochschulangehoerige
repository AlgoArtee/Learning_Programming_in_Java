public class Hochschulangehoerige
{
    // Instanzvariablen - ersetzen Sie das folgende Beispiel mit Ihren Variablen
    protected String vorname;
    protected String nachname;
    protected String adresse;

    /**
     * Konstruktor für Objekte der Klasse Hochschulangehoerige
     */
    public Hochschulangehoerige(String vorname, String nachname, String adresse)
    {
        // Instanzvariable initialisieren
        this.vorname = vorname;
        this.nachname = nachname;
        this.adresse = adresse;
    }


    public String toString()
    {
        return "Die Hochschulangehörige heißt " + vorname + " " + nachname + " und wohnt in " + adresse;
    }
}
