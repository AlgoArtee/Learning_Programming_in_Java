public class Mitarbeiterin8_1 extends Hochschulangehoerige
{
    // Instanzvariablen - ersetzen Sie das folgende Beispiel mit Ihren Variablen
    private int personalnummer;

    
    public Mitarbeiterin8_1(String vorname, String nachname, String adresse, int personalnummer)
    {
     super(vorname, nachname, adresse);
     this.personalnummer = personalnummer;
    
    }

    public String getName()
    {
        return nachname;
    }
    
    
    public void setName(String name)
    {
        this.nachname = nachname;
    }
    
    
    public int getPersonalnummer()
    {
        return personalnummer;
    }
    
    
    
    public String toString()
    {
        return "Die Mitarbeiterin hei√üt " + vorname + " " + nachname + " und hat die Personalnummer " + personalnummer + ". Sie wohnt in " + adresse;
    
    }
    
}
